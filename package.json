{
    "name": "hp-chemstation-gc-ms-macro-language",
    "displayName": "HP Chemstation GC/MS Macro Language",
    "description": "Syntax highlighting and language support.",
    "publisher": "AndrewSchult",
    "icon": "images/ICON.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/MorselHoarder/hp-chemstation-gc-ms-macro-language.git"
    },
    "bugs": {
        "url": "https://github.com/MorselHoarder/hp-chemstation-gc-ms-macro-language/issues",
        "email": "schult.andrew@gmail.com"
    },
    "homepage": "https://github.com/MorselHoarder/hp-chemstation-gc-ms-macro-language/blob/master/README.md",
    "version": "1.1.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "agilent",
        "hewlett-packard",
        "masshunter",
        "chemstation",
        "chromatography"
    ],
    "contributes": {
        "languages": [
            {
                "id": "chemmac",
                "aliases": [
                    "Chemstation Macro",
                    "chemmac",
                    "MSD Chemstation GC/MS"
                ],
                "extensions": [
                    ".mac"
                ],
                "configuration": "./chemmac-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "chemmac",
                "scopeName": "source.chemmac",
                "path": "./syntaxes/chemmac.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.14.0"
    }
}
