						"match": "\\\\[#%\\\\]"
					}
					]
				},
				{
					"name": "string.quoted.single.chemmac",
					"begin": "'",
					"end": "'",
					"patterns": [{
						"name": "constant.character.accelerator.chemmac",
						"match": "&[a-zA-Z0-9]"
					},
					{
						"name": "constant.character.escape.chemmac",
						"match": "\\\\[#%\\\\]"
					}
					]
				}
			]
		},
		"stringsNested": {
			"patterns": [
					{
					"name": "string.quoted.other.chemmac",
					"begin": "`",
					"end": "'",
					"patterns": [{
						"name": "constant.character.accelerator.chemmac",
						"match": "&[a-zA-Z0-9]"
					},
					{
						"name": "constant.character.escape.chemmac",
						"match": "\\\\[#%\\\\]"
					},
					{"include": "#stringsNested"}
					]
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "constant.numeric.chemmac",
					"match": "(?<=([\\s\\[\\=\\(#,:<>+*/]))[\\-]?[0-9]+[\\.]?[0-9]*(?:[eE][\\+\\-]?[0-9]+)?"
				},
				{
					"name": "constant.numeric.chemmac",
					"match": "(?<=(-))[0-9]+[\\.]?[0-9]*(?:[eE][\\+\\-]?[0-9]+)?"
				},
				{
					"name": "constant.numeric.chemmac",
					"match": "(?<=([\\s\\[\\=\\(#,:<>+*/]))[\\-]?[\\.][0-9]+(?:[eE][\\+\\-]?[0-9]+)?"
				},
				{
					"name": "keyword.operator.chemmac",
					"match": "(?:[=<>][=<>]?)|(?:\\+)|(?:\\-)|(?:/)|(?:\\*)|(?:\\:)|(?:\\b(?i:(and|or|not))\\b)"
				},
				{
					"match": ",",
					"name": "punctuation.separator.chemmac"
				},
				{
					"match": "(?:\\[)|(?:\\])|(?:\\()|(?:\\))",
					"name": "punctuation.brace.chemmac"
				},
				{
					"match": "#",
					"name": "constant.character.chemmac"
				}
			]
		},
		"comments": {
			"name": "comment.line.exclamation.chemmac",
			"begin": "!",
			"end": "\\n"
		},
		"entity": {
			"patterns": [{
				"match": "^[\\s]*(?i:(name))[\\s]+?([a-zA-Z_][a-zA-Z0-9_]*[\\$]?)",
				"captures": {
					"1": {
						"name": "storage.modifier.macro.chemmac"
					},
					"2": {
						"name": "entity.name.type.macro.chemmac"
					}
				}
			}
			]
		},
		"storage": {
			"patterns":[{
				"begin": "^[\\s]*?(?i:(parameter))\\s",
				"beginCaptures": {
					"1": {
						"name": "storage.modifier.parameter.chemmac"
					}
				},
				"end": "$",
				"patterns": [{
					"name": "keyword.operator.chemmac",
					"match": "\\b(?i:(default|def|d|byname))\\b"
				},
				{
					"match": "(?<=,)[\\s]*([a-zA-Z_][a-zA-Z0-9_]*[\\$]?)",
					"captures": {
						"1": {
							"name": "variable.parameter.chemmac"
						}
					}
				},
				{
					"match": "(?<=(?i:parameter)[\\s])[\\s]*([a-zA-Z_][a-zA-Z0-9_]*[\\$]?)",
					"captures": {
						"1": {
							"name":"variable.parameter.chemmac"
						}
					}
				},
				{ "include": "#staticRules" },
				{"include": "#keywords"},
				{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*?(?i:(local))\\s",
				"beginCaptures": {
					"1": {
					"name": "storage.modifier.chemmac"
					}
				},
				"end": "$",
				"patterns": [
				{ "include": "#staticRules" },
				{"include": "#keywords"},
				{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*(?i:(remove))\\s",
				"beginCaptures": {
					"1": {
						"name": "storage.modifier.chemmac"
					}
				},
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{
						"match": "[_a-zA-Z][_a-zA-Z0-9]*[\\$]?",
						"name": "entity.name.chemmac"
					}
				]
			}
			]
		},
		"statements": {
			"patterns": [
			{"include": "#commandList"},
			{"include": "#reservedVariables"},
			{"include": "#systemVariables"},
			{
				"begin": "^[\\s]*([a-zA-Z_][a-zA-Z0-9_]*[\\$]?)\\s++(?![\\[\\=])",
				"beginCaptures": {
					"1": {
						"name": "entity.name.class.macro.chemmac"
					}
				},
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{"include": "#variables"}
				]
			},
			{
				"begin": "^(?=[\\s]*+([a-zA-Z_][a-zA-Z0-9_]*+[\\$]?)[\\s]*+[\\[\\=])",
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{"include": "#variables"}
				]
			}
			]
		},
		"staticRules": {
			"patterns": [
				{
					"include": "#strings"
				},
				{
					"include": "#stringsNested"
				},
				{
					"include": "#constants"
				},
				{
					"include": "#comments"
				}
			]
		}
	}
}