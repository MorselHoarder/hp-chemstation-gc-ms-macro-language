		"logic": {
			"patterns": [{
				"begin": "^[\\s]*(?i:(if))(?![\\w\\$])",
				"beginCaptures": {
					"1": {
						"name": "keyword.control.chemmac"
					}
				},
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*(?i:(while))(?![\\w\\$])",
				"beginCaptures": {
					"1": {
						"name": "keyword.control.chemmac"
					}
				},
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*(?i:(endwhile|endif|else|return))(?![\\w\\$])",
				"beginCaptures": {"1": {"name": "keyword.control.chemmac"}},
				"end": "(?=\\!)|$",
				"patterns": [
					{"include": "#comments"},
					{
						"match": "[\\S]+",
						"name": "invalid.illegal.chemmac"
					}
				]
			},
			{
				"begin": "^[\\s]*(?i:(macro|runmac))(?![\\w\\$])",
				"beginCaptures": {"1": {"name": "storage.modifier.function.chemmac"}},
				"end": "(?=\\!)|$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{
						"match": "(?<=[\\s,])(?i:(g|go))\\b",
						"name": "keyword.other.chemmac"
					},
					{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*(?i:(on[ _]error))(?![\\w\\$])",
				"beginCaptures": {
					"1": {
						"name": "keyword.control.chemmac"
					}
				},
				"end": "$",
				"patterns": [
					{"include": "#keywords"},
					{"include": "#staticRules"},
					{
						"name": "support.function.chemmac",
						"match": "[_a-zA-Z][_a-zA-Z0-9]*[\\$]?[\\s]+(?![\\[\\=])"
					},
					{"include": "#variables"}
				]
			},
			{
				"begin": "^[\\s]*(?i:(generate[ _]error))(?![\\w\\$])",
				"beginCaptures": {
